@model IEnumerable<ProjetFinal_6224745.Models.Agre>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nom)
            </th>
            <th>
                Nombre de mouvement
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AgreId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AgreId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AgreId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
